{
  "name": "princejs",
  "version": "1.4.2",
  "description": "An easy and fast backend framework â€” by a 13yo developer, for developers.",
  "main": "dist/prince.js",
  "types": "dist/prince.d.ts",
  "exports": {
    ".": {
      "import": "./dist/prince.js",
      "types": "./dist/prince.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./dist/validation.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun && bun build src/middleware.ts --outdir dist --target bun && bun build src/validation.ts --outdir dist --target bun",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "backend",
    "api",
    "framework",
    "bun",
    "easy",
    "princejs",
    "rest",
    "server",
    "typescript"
  ],
  "author": "Matthew Michael (MatthewTheCoder1218)",
  "license": "MIT",
  "repository": "https://github.com/MatthewTheCoder1218/princejs",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "bun-types": "latest",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  }
}
